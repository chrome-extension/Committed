{
  "name": "Committed - NewTab Todo and Goal Manager",
  "short_name": "Committed",
  "description": "Manage your weekly goals and todos in your newtab",
  "version": "2.0.0",
  "manifest_version": 2,
   "chrome_url_overrides" : {
     "newtab": "index.html"
   },
   "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ]
}
